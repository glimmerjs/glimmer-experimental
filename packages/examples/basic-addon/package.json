{
  "name": "basic-addon",
  "version": "0.6.8",
  "description": "The default blueprint for ember-cli addons.",
  "private": true,
  "keywords": [
    "ember-addon"
  ],
  "repository": "",
  "license": "MIT",
  "author": "",
