    "ember-resolver": "^7.0.0",
    "ember-source": "~3.26.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.4.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.29.0",
    "eslint-plugin-ember": "^8.1.1",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.18.1",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.72.0"
  },
  "engines": {
    "node": "10.* || >= 12"
