{
  "name": "@glimmerx/storybook",
  "version": "0.6.8",
  "description": "storybook for glimmerjs",
  "keywords": [
    "storybook",
    "glimmerjs"
  ],
  "authors": [
    "Abhishek Aroskar <abhi.aroskar.github@gmail.com> (https://github.com/aroskar)",
    "Jonathan Tsang <jonathantsangemail@gmail.com> (https://github.com/jonathantsang)"
  ],
  "license": "MIT",
  "files": [
    "bin/**/*",
    "dist/**/*",
    "README.md",
    "*.js"
  ],
  "main": "dist/client/index.js",
  "bin": {
    "build-storybook": "./bin/build.js",
    "start-storybook": "./bin/index.js",
    "storybook-server": "./bin/index.js"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./dist --config-file ./.babelrc.js --copy-files --extensions '.js,.jsx,.ts,.tsx'",
    "ts-compile": "tsc -p ."
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-typescript": "^7.9.0",
    "@types/node": "^12.1.0",
    "@types/webpack": "^4.41.10",
    "@types/webpack-env": "^1.15.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.4",
    "eslint": "^7.29.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@storybook/core": "^6.4.1",
    "@storybook/addons": "^6.4.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "global": "^4.4.0"
  },
  "peerDependencies": {
    "@glimmerx/babel-preset": "^0.6.8",
    "@glimmerx/component": "^0.6.8",
    "@glimmerx/core": "^0.6.8",
    "@glimmerx/webpack-loader": "^0.6.8",
    "babel-loader": "^8.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "volta": {
    "node": "12.10.0",
    "yarn": "1.22.4"
  }
}
